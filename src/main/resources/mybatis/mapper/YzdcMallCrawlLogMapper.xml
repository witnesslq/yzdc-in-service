<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.yzdc.api.dao.YzdcMallCrawlLogDAO">
    <resultMap id="IfFinishedMap" type="java.util.HashMap">
        <result column="is_finishded" property="is_finishded"/>
    </resultMap>
    <!-- 读取日志表,判断数据同步是否完成 -->
    <select id="getSynFlagByType" resultMap="IfFinishedMap">
        select is_finishded
        from yz_kb_crawl_log
        where biz_date = #{reportDate,jdbcType=VARCHAR} and busi_type in
        <foreach item="item" index="index" collection="types" open="(" separator="," close=")">
            #{item}
        </foreach>
    </select>
</mapper>